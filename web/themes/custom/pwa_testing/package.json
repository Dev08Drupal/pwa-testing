{
  "name": "pwa_testing",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "init": "node init-structure.js",
    "sass-dev": "sass --watch scss:css --style expanded --source-map",
    "sass-prod": "sass scss:css --style compressed --no-source-map",
    "sass-compressed": "sass --watch scss:css --style compressed --no-source-map",
    "postcss": "postcss css/**/*.css --use autoprefixer -d css/ --map",
    "postcss-prod": "postcss css/**/*.css --use autoprefixer -d css/ --no-map",
    "postcss-compressed": "postcss css/**/*.css --use autoprefixer -d css/ --map",
    "js-dev": "webpack --mode development --watch",
    "js-prod": "webpack --mode production",
    "js-compressed": "webpack --mode production --watch",
    "build-css": "npm run sass-prod && npm run postcss-prod",
    "build-js": "npm run js-prod",
    "build": "npm run build-css && npm run build-js",
    "dev": "concurrently \"npm run sass-dev\" \"npm run js-dev\"",
    "dev-compressed": "concurrently \"npm run sass-compressed\" \"npm run postcss-compressed\" \"npm run js-compressed\"",
    "watch": "concurrently \"npm run sass-dev\" \"webpack --mode development --watch\"",
    "watch-compressed": "concurrently \"npm run sass-compressed\" \"webpack --mode production --watch\"",
    "clean": "rm -rf css/**/*.css css/**/*.map js/dist/**/*.js js/dist/**/*.map"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@popperjs/core": "^2.11.8",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "bootstrap": "^5.3.3",
    "concurrently": "^8.2.2",
    "glob": "^10.3.10",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "sass": "^1.80.7",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  }
}
